{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "jsxSingleQuote": true,
                "no-console": true,
                "trailingComma": "es5",
                "semi": true,
                "singleQuote": true,
                "useTabs": true,
                "template-curly-spacing": false
            }
        ]
    }
}
